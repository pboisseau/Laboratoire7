#include <iostream>
using namespace std;

long long factorielle(int n);

int main(void)
{
	cout << factorielle(1) << endl;
	return 0;
}

long long factorielle(int n)
{
	long long result = 1;

	for(int i = n; i <= 1; i--)
	{
		result = result * i;
	}

	return result;
}
